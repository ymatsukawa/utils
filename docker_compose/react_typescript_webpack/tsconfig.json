/*
allowSyntheticDefaultImports:
https://www.typescriptlang.org/tsconfig#allowSyntheticDefaultImports
*/
{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    "allowSyntheticDefaultImports": true
  }
}
